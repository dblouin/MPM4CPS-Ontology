    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>#PROMISEDSL</IRI>
        <Literal>key:PROMISE-website</Literal>
    </AnnotationAssertion>
